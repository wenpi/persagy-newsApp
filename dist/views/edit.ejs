<%- include header %>
	<form class="form-horizontal" role="form">
	  <div class="form-group">
	    <label for="inputTitle" class="col-sm-2 control-label">数据</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" id="inputTitle" placeholder="请添加数据">
	    </div>
	  </div>
	  <div class="form-group">
	    <label for="inputSign" class="col-sm-2 control-label">符号</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" id="inputSign" placeholder="+">
	    </div>
	  </div>
	  <div class="form-group">
	    <label for="inputUnit" class="col-sm-2 control-label">单位</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" id="inputUnit" placeholder="%">
	    </div>
	  </div>
	  <div class="form-group">
	    <label for="inputSubtitle" class="col-sm-2 control-label">副标题</label>
	    <div class="col-sm-10">
		  <textarea class="form-control" rows="3" id="inputSubtitle" placeholder="本周空调末端比去年同期增加65%"></textarea>
	    </div>
	  </div>
	  <div class="form-group">
	    <label for="inputText" class="col-sm-2 control-label">正文</label>
	    <div class="col-sm-10">
		  <textarea class="form-control" rows="8" id="inputText" ></textarea>
	    </div>
	  </div>
	  <div class="form-group">
	    <label for="inputText" class="col-sm-2 control-label">发布对象</label>
	    <div class="col-sm-10">
		  <textarea class="form-control" rows="8" id="inputText" ></textarea>
	    </div>
	  </div>
	  <div class="form-group">
	    <div class="col-sm-offset-2 col-sm-10">
	      <button type="submit" class="btn btn-primary">保存</button>
	    </div>
	  </div>
	</form>
<%- include footer %>